    <hangDetector>
        <enable>false</enable>
        <hangUpdateDelay>4000</hangUpdateDelay>
        <hangFailCount>10</hangFailCount>
    </hangDetector>
    <fpsgathering>true</fpsgathering>
    <customization>
        <currentRealm>asia</currentRealm>
    </customization>
    <webBrowser>
        <processExePath>cef/cef_browser_process.exe</processExePath>
        <userAgent>Mozilla/5.0 (Windows NT 6.1; WOW64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/39.0.2171.95 Safari/537.36 WOWS/1.0</userAgent>
        <cachePath>cef_cache</cachePath>
        <hardwareAcceleration>false</hardwareAcceleration>
        <maxFrameRate>30</maxFrameRate>
        <logVerbosity>info</logVerbosity>
        <enableStatistics/>
        <debugLog>debug.log</debugLog>
        <pathBase>EXE_PATH</pathBase>
        <dirPath>profile</dirPath>
    </webBrowser>
    <window>
        <iconBlinkPeriod>0</iconBlinkPeriod>
    </window>
    <scriptLogger>
        <fileSizeInMb>5</fileSizeInMb>
        <volumesCount>2</volumesCount>
        <dirPath>profile</dirPath>
        <pathBase>EXE_PATH</pathBase>
    </scriptLogger>
    <memleaks>
        <dirPath>profile</dirPath>
        <name>memleaks.log</name>
        <pathBase>EXE_PATH</pathBase>
    </memleaks>
    <allocations>
        <dirPath>allocations</dirPath>
        <name>allocation_stats.log</name>
